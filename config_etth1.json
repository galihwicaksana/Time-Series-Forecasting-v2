{
	"data": {
		"filename": "dataset/ETT-small/ETTh1.csv",
		"columns": [
			"HUFL",
			"HULL",
			"MUFL",
			"MULL",
			"LUFL",
			"LULL",
			"OT"
		],
		"sequence_length": 96,
		"train_test_split": 0.7,
		"normalise": true
	},
	"training": {
		"epochs": 50,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "checkpoints/lstm_etth1",
		"layers": [
			{
				"type": "lstm",
				"neurons": 128,
				"input_timesteps": 95,
				"input_dim": 7,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 64,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 32,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
